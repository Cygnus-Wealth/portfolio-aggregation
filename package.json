{
  "name": "@cygnus-wealth/portfolio-aggregation",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "description": "Portfolio orchestration and aggregation service library for CygnusWealth client applications",
  "keywords": [
    "domain:portfolio",
    "context:aggregation",
    "type:service-library",
    "cygnus-wealth",
    "ddd",
    "portfolio-management",
    "orchestration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cygnus-wealth/portfolio-aggregation"
  },
  "homepage": "https://github.com/cygnus-wealth/portfolio-aggregation#readme",
  "bugs": {
    "url": "https://github.com/cygnus-wealth/portfolio-aggregation/issues"
  },
  "author": "CygnusWealth",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b && vite build --mode library",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:once": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules .turbo",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@cygnus-wealth/data-models": "^1.0.0",
    "@cygnus-wealth/evm-integration": "^1.0.0",
    "@cygnus-wealth/sol-integration": "^1.0.0",
    "@cygnus-wealth/robinhood-integration": "^1.0.0",
    "@cygnus-wealth/asset-valuator": "^1.0.0"
  },
  "peerDependencies": {
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
